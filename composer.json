{
    "name": "songshenzong/git",
    "description": "A Git Client for PHP",
    "keywords": [
        "git",
        "client"
    ],
    "license": "MIT",
    "homepage": "http://songshenzong.com",
    "support": {
        "issues": "https://github.com/songshenzong/git/issues",
        "source": "https://github.com/songshenzong/git"
    },
    "authors": [
        {
            "name": "Songshenzong",
            "email": "i@songshenzong.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "danielstjules/stringy": "^3.1",
        "songshenzong/command": "^1.0"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4",
        "phpunit/phpunit": "^4.8.35|^5.4.3",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Songshenzong\\Git\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Songshenzong\\Git\\Tests\\": "tests"
        }
    },
    "scripts": {
        "fixer": "php-cs-fixer fix ./"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
